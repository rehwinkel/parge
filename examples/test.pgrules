token LIT_HEX = ("0x" [0-F])+;
token LIT_OCT = ("0o" [0-7])+;
token LIT_BIN = ("0b" [01])+;
token LIT_FLOAT = ([0-9])+ "." ([0-9])+;
token KW_FUNC = "func";
token KW_STRUCT = "struct";
token KW_BYTE = "byte";
token KW_SHORT = "short";
token KW_INT = "int";
token KW_LONG = "long";
token KW_FLOAT = "float";
token KW_DOUBLE = "double";
token LIT_CHAR = "'" [^'] "'";
